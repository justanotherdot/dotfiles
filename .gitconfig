[include]
  path = /home/rjs/.gitcreds
[push]
	default = simple
[alias]
  s       = status --short --branch
  l       = log --oneline
  c       = commit
  b       = branch
  co      = checkout
  com     = checkout master
  pom     = pull origin master
  new     = '!git init && git commit -m "root" --allow-empty'
  jump    = "!f() { git stash; git checkout $1; git stash drop; }; f"
  slap    = stash apply
  spill   = "!git stash && git stash drop"
  squish  = "!git commit --fixup HEAD && git rebase --autosquash HEAD~2"
	commend = commit --amend --no-edit
[core]
	excludesfile = /home/rjs/.gitignore_global
